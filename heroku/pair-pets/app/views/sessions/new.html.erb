<div class="login">
<div class="container">
	<%= form_tag login_path do %>
		<label> Username: </label><br>
		<%= text_field_tag :'user[username]' %><br>
		<label> Password: </label><br>
		<%= password_field_tag :'user[password]' %>
		<%= submit_tag "Log In" %>
	<% end %>

	Don't have an account?
	<%= link_to "Sign Up", new_user_path %>
</div>
</div>