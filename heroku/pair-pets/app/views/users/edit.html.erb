<div class="container">
	<h3>Edit Profile </h3>
	<%= form_for :user do |f| %>
		<%= f.label :username %>
		<%= f.text_field :username %>
		<%= f.label :password %>
		<%= f.password_field :password %>
		<%= f.label :password_confirmation %>
		<%= f.password_field :password_confirmation %>
		<%= f.label :city %>
		<%= f.text_field :city %>
		<%= f.label :bio %>
		<%= f.text_field :description %>
		<%= f.label :image %>
		<%= f.file_field :image %>
		<%= f.submit "Update User" %>
	<% end %>
</div>